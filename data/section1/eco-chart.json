{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Unemployment rate",
    "subtitle": "Ages 16 and over, % | Source: ONS through ECO API",
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "royalBlue",
    "subtitleColor": "#000000"
  },
  "height": "150",
  "width": "320",
  "background": "#ff00ff",
  "view": {
    "fill": "#00ffff"
  },
  "data": {
    "url": "https://api.economicsobservatory.com/gbr/unem?vega",
    "format": {
      "type": "json"
    }
  },
  "mark": {
    "type": "area",
    "color": "#f4134d",
    "strokeWidth": "2"
  },
  "transform": [{
    "filter":"year(datum.date)>2019"
  }],
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal"
      
    },
    "y": {
      "field": "value",
      "type": "quantitative"
      
    }
  }
}
